# ===========================================
# AI图片生成器 - 环境变量配置
# ===========================================

# 应用基础配置
NODE_ENV=production
NEXTAUTH_URL=https://your-domain.com
JWT_SECRET=your-super-secure-jwt-secret-key-here

# ===========================================
# Supabase 数据库配置
# ===========================================
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# ===========================================
# 阿里云短信服务配置
# ===========================================
ALICLOUD_ACCESS_KEY_ID=your-alicloud-access-key-id
ALICLOUD_ACCESS_KEY_SECRET=your-alicloud-access-key-secret
ALICLOUD_SMS_SIGN_NAME=your-sms-sign-name
ALICLOUD_SMS_TEMPLATE_CODE=SMS_123456789

# ===========================================
# 微信登录配置
# ===========================================
# 微信开放平台应用配置
WECHAT_APP_ID=wx1234567890abcdef
WECHAT_APP_SECRET=your-wechat-app-secret

# 微信网站应用配置（用于PC端扫码登录）
WECHAT_WEB_APP_ID=wx1234567890abcdef
WECHAT_WEB_APP_SECRET=your-wechat-web-app-secret

# ===========================================
# 支付宝登录配置
# ===========================================
# 支付宝开放平台应用配置
ALIPAY_APP_ID=20211234567890
ALIPAY_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nMIIEvgIBADANBgkqhkiG9w0BAQEFAASCBKgwggSkAgEAAoIBAQC...\n-----END PRIVATE KEY-----"
ALIPAY_PUBLIC_KEY="-----BEGIN PUBLIC KEY-----\nMIIBIjANBgkqhkiG9w0BAQEFAAOCAQ8AMIIBCgKCAQEA...\n-----END PUBLIC KEY-----"

# 支付宝平台公钥（用于验证支付宝返回的数据）
ALIPAY_PLATFORM_PUBLIC_KEY="-----BEGIN PUBLIC KEY-----\nMIIBIjANBgkqhkiG9w0BAQEFAAOCAQ8AMIIBCgKCAQEA...\n-----END PUBLIC KEY-----"

# ===========================================
# 第三方登录回调域名配置
# ===========================================
# 生产环境回调域名
OAUTH_CALLBACK_DOMAIN=https://your-domain.com

# ===========================================
# 可选：其他第三方登录服务
# ===========================================
# GitHub OAuth配置
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# Google OAuth配置
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret

# QQ登录配置
QQ_APP_ID=123456789
QQ_APP_KEY=your-qq-app-key

# ===========================================
# 安全和性能配置
# ===========================================
# Session密钥
SESSION_SECRET=your-session-secret-key

# Redis缓存配置（可选，用于session存储）
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=your-redis-password

# 限流配置
RATE_LIMIT_ENABLED=true
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_WINDOW_MS=900000

# ===========================================
# 监控和日志配置（可选）
# ===========================================
# 错误监控（如Sentry）
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# 分析服务
ANALYTICS_ID=GA-XXXXXXXXX

# ===========================================
# 邮件服务配置（可选）
# ===========================================
# SMTP配置
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=your-email@example.com
SMTP_PASS=your-email-password
SMTP_FROM=your-app-name <noreply@your-domain.com>

# ===========================================
# 文件存储配置（可选）
# ===========================================
# 阿里云OSS
OSS_ACCESS_KEY_ID=your-oss-access-key-id
OSS_ACCESS_KEY_SECRET=your-oss-access-key-secret
OSS_BUCKET=your-oss-bucket
OSS_REGION=oss-cn-hangzhou
OSS_ENDPOINT=https://oss-cn-hangzhou.aliyuncs.com

# AWS S3
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_REGION=us-east-1
AWS_BUCKET=your-s3-bucket

# ===========================================
# 支付配置（可选）
# ===========================================
# 微信支付
WECHAT_PAY_MCHID=your-wechat-pay-merchant-id
WECHAT_PAY_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----"
WECHAT_PAY_SERIAL_NO=your-certificate-serial-number

# 支付宝支付
ALIPAY_PAY_APP_ID=your-alipay-pay-app-id
ALIPAY_PAY_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----"

# ===========================================
# AI服务配置
# ===========================================
# OpenAI API配置
OPENAI_API_KEY=sk-your-openai-api-key
OPENAI_BASE_URL=https://api.openai.com/v1

# Replicate API配置
REPLICATE_API_TOKEN=r8_your-replicate-api-token

# Hugging Face配置
HUGGINGFACE_API_TOKEN=hf_your-huggingface-token 